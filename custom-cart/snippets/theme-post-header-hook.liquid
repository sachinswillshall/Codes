</head>

{% comment %}
  Add the page template as a class for easy page or template specific styling.
{% endcomment %}
<body id="{{ langify_title | handle }}" class="{% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}" >

  <div id="NavDrawer" class="drawer drawer--left{% if theme-post-header-hook %} {{theme-post-header-hook}}{% endif %}">
    <!-- begin mobile-nav -->
    <div class="mobile-nav">
      <div class="mobile-nav__main">
        <header>
          <button class="js-drawer-close drawer__close icon-fallback-text">
            <span class="icon icon-hamburger" aria-label="{{ 'layout.drawers.close_menu' | t }}"></span>
            <span class="fallback-text" aria-hidden="true">{{ 'layout.drawers.close_menu' | t }}</span>
          </button>
          <div class="mobile-nav__logo-wrapper">
            <a href="/" class="mobile-nav__logo-link">
              <img src="{{ 'logo.svg' | asset_url }}" alt="{{ {{
shop.name }}">
            </a>
          </div>
        </header>
        <ul class="mobile-nav__list">
          {% assign display_icon = true %}
          {% assign linklist = linklists.main-menu %}
          {% include 'drawer-linklist' %}
        </ul>
        <ul class="mobile-nav__list">
          {% assign linklist = linklists.mobile-main %}
          {% include 'drawer-linklist' %}
        </ul>
        <div class="relative">
          <ul class="mobile-nav__list">
        {% comment %}    <li class="mobile-nav__item">
              {% include 'ly-languages-switcher' %}
            </li>{% endcomment %}
            {% include 'social-links' %}
          </ul>



        </div>
      </div>
      <div class="mobile-nav__minor" style="margin-top:47px">
        <div class="grid--full">
          <div class="grid__item three-fifths">
            <span class="mobile-nav__minor--text">Â© {{ 'now' | date: "%Y" }} {{ {{
shop.name }}</span>
          </div>
          <div class="grid__item two-fifths text-right">
            <a data-toggle-mobile-minor-links href="#all_links" class="mobile-nav__minor--text">
              <span data-mobile-minor-more-label>
                <span>{{ 'layout.drawers.see_all_links' | t }}</span>
                <span class="icon-down-open"></span>
              </span>
              <span data-mobile-minor-less-label style="display: none;">
                <span>{{ 'layout.drawers.see_less' | t }}</span>
                <span class="icon-up-open"></span>
              </span>
            </a>
          </div>
        </div>
        <ul data-mobile-minor-links class="mobile-nav__list--minor" style="display: none;">
          {% assign display_icon = false %}
          {% assign linklist = linklists.mobile-minor %}
          {% include 'drawer-linklist' %}
        </ul>
        <div id="all_links"></div>
      </div>
    </div>
    <!-- //mobile-nav -->
  </div>
  {% if settings.ajax_cart_method == "drawer" %}
    <div id="CartDrawer" class="drawer drawer--right{% if theme-post-header-hook %} {{theme-post-header-hook}}{% endif %}">
      <div id="CartContainer"></div>
      <a href="#" class="js-drawer-close drawer__close minicart-secondary">
        {{ 'cart.general.continue_browsing' | t }}
      </a>
    </div>
  {% endif %}
  <div id="PageContainer">
